{
  "JwtSettings": {
    "Issuer": "${{ secrets.JWT_ISSUER }}",
    "Audience": "${{ secrets.JWT_AUDIENCE }}",
    "Key": "${{ secrets.JWT_KEY }}"
  },

  "MySql": {
    "MySqlRead": "${{ secrets.MYSQL_READ }}",
    "MySqlWrite": "Server=${{ secrets.MYSQL_WRITE }}",
    "MySqlReadWrite": "Server=${{ secrets.MYSQL_READWRITE }}",
    "MySqlAdmin": "Server=${{ secrets.MYSQL_ADMIN }}"
  },

  "MongoDB": {
    "Connection": "${{ secrets.MONGODB_CONN }}"
  },

  "Neo4jSettings": {
    "Neo4jConnection": "${{ secrets.NEO4J_CONN }}",
    "Neo4jUser": "${{ secrets.NEO4J_USER }}",
    "Neo4jPassword": "${{ secrets.NEO4J_PASSWORD }}",
    "Neo4jDatabase": "${{ secrets.NEO4J_DATABASE }}"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}